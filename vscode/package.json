{
	"name": "ui5-i18n-plugin",
	"displayName": "UI5 i18n Check",
	"description": "Check your UI5 project for missing translations!",
	"version": "0.0.4",
	"publisher": "fatihpense",
	"engines": {
		"vscode": "^1.44.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mdpgroup/ui5-i18n-plugin"
	},
	"icon": "images/icon.png",
	"license": "MIT",
	"categories": [
		"Other"
	],
	"keywords": [
		"openui5",
		"fiori",
		"sap",
		"ui5",
		"internationalization",
		"i18n"
	  ],
	"activationEvents": [
		"onCommand:ui5-i18n-plugin.check-translations"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ui5-i18n-plugin.check-translations",
				"title": "Check i18n Translations"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"xmldom": "^0.5.0"
	}
}
